{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@storybook/*": [".storybook/*"],
      "@/*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@img/*": ["src/assets/img/*"],
      "@scss/*": ["src/assets/scss/*"],
      "@components/*": ["src/components/*"],
      "@routes": ["src/routes"],
      "@views/*": ["src/views/*"],
      "@mock/*": ["src/mock/*"]
    }
  }
}
